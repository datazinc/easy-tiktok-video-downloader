{
  "name": "Easy TikTok Profile Download",
  "description": "Extract liked videos and any profile video links like a pro!",
  "version": "1.5",
  "manifest_version": 3,
  "host_permissions": ["*://*.tiktok.com/"],
  "permissions": ["activeTab"],
  "web_accessible_resources": [
    {
      "resources": ["injex.js"],
      "matches": ["*://*.tiktok.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["*://*.tiktok.com/*"],
      "js": ["content.js"],
      "css": ["injex.css"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "32": "icons/icon32.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
